#!/bin/bash

if [[ $(pamixer --get-mute) = "false" ]]; then
	volume=$(pamixer --get-volume) 
	
	if [[ $volume -le 33 ]]; then
		echo $(printf "ðŸ”ˆ %d%%" $volume)
	elif [[ $volume -le 66 ]]; then
		echo $(printf "ðŸ”‰ %d%%" $volume)
	else
		echo $(printf "ðŸ”Š %d%%" $volume)
	fi
else
	echo "ðŸ”‡"
fi

